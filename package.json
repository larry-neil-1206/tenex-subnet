{
  "name": "tenex",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx hardhat test test/local/TenexiumProtocol.test.ts",
    "test-getters": "npx hardhat run test/testnet/getters.ts",
    "test-setters": "npx hardhat run test/testnet/setters.ts",
    "test-liquidity": "npx hardhat run test/testnet/liquidity.ts",
    "test-subnet-manager": "npx hardhat run test/testnet/subnet_manager.ts",
    "test-address-conversion": "npx hardhat run test/testnet/address_conversion.ts",
    "test-contract-check": "npx hardhat run test/testnet/contract_check.ts",
    "build": "npx hardhat compile",
    "deploy:local": "npx hardhat deploy:new_proxy --save --network local",
    "deploy:testnet": "npx hardhat deploy:new_proxy --save --network testnet",
    "deploy:mainnet": "npx hardhat deploy:new_proxy --save --network mainnet",
    "deploy:local:implementation": "npx hardhat deploy:implementation --save --network local",
    "deploy:testnet:implementation": "npx hardhat deploy:implementation --save --network testnet",
    "deploy:mainnet:implementation": "npx hardhat deploy:implementation --save --network mainnet",
    "upgrade:local": "npx hardhat upgrade:proxy --save --network local",
    "upgrade:testnet": "npx hardhat upgrade:proxy --save --network testnet",
    "upgrade:mainnet": "npx hardhat upgrade:proxy --save --network mainnet",
    "deploy:testnet:subnet-manager:new-proxy": "npx hardhat deploy:subnet-manager:new_proxy --save --network testnet",
    "deploy:mainnet:subnet-manager:new-proxy": "npx hardhat deploy:subnet-manager:new_proxy --save --network mainnet",
    "deploy:testnet:subnet-manager:implementation": "npx hardhat deploy:subnet-manager:implementation --save --network testnet",
    "deploy:mainnet:subnet-manager:implementation": "npx hardhat deploy:subnet-manager:implementation --save --network mainnet",
    "upgrade:testnet:subnet-manager:proxy": "npx hardhat upgrade:subnet-manager:proxy --save --network testnet",
    "upgrade:mainnet:subnet-manager:proxy": "npx hardhat upgrade:subnet-manager:proxy --save --network mainnet",
    "deploy:testnet:address-conversion": "npx hardhat deploy:address-conversion --save --network testnet",
    "deploy:mainnet:address-conversion": "npx hardhat deploy:address-conversion --save --network mainnet",
    "storage-check": "npx hardhat clean && npx hardhat storage-layout --check",
    "storage-update": "npx hardhat clean && npx hardhat storage-layout --update",
    "anvil": "anvil -p 8545",
    "forge:test": "forge test -vvv",
    "forge:gas": "forge test --gas-report",
    "coverage": "hardhat coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.14",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.0",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@typechain/ethers-v6": "^0.5.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "chai": "^4.2.0",
    "dotenv": "^17.2.1",
    "ethers": "^6.14.0",
    "hardhat": "^2.26.3",
    "hardhat-contract-sizer": "^2.10.1",
    "hardhat-gas-reporter": "^2.3.0",
    "hardhat-storage-layout": "^0.1.7",
    "install": "^0.13.0",
    "npm": "^11.5.2",
    "solidity-coverage": "^0.8.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@polkadot/util": "^13.5.6",
    "@polkadot/util-crypto": "^13.5.6",
    "ethers-decode-error": "^2.1.3",
    "hardhat-storage-layout-changes": "^0.1.2"
  }
}
